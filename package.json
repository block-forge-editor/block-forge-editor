  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "style": "./dist/index.css"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "dev": "vite dev",
    "packages:upgrade": "ncu --interactive --format group && npm i --force",
    "report:find-deadcode": "ts-prune",
    "report:tests": "vitest --coverage --ui",
    "report:circular": "npx dpdm  --no-warning --no-tree -o deps.json ./src/index.ts",
    "fix:prettier": "prettier --write \"./src/**/*.{js,mjs,cjs,ts,jsx,tsx}\"",
    "fix:eslint": "eslint \"./src/**/*.{js,mjs,cjs,ts,jsx,tsx}\" --fix ."
