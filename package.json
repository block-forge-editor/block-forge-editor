{
  "name": "@block-forge/block-forge-editor",
  "version": "1.0.1",
  "description": "A React component library with modern UI blocks",
  "private": false,
  "license": "MIT",
  "author": "alkhipce",
  "repository": {
    "type": "git",
    "url": "https://github.com/block-forge-editor/block-forge-editor.git"
